{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "anik/form-request": "^4.5",
        "bacon/bacon-qr-code": "^2.0",
        "bensampo/laravel-enum": "^5.3",
        "dusterio/lumen-passport": "^0.3.6",
        "flipbox/lumen-generator": "^9.1",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/mail": "^9.17",
        "illuminate/notifications": "^9.17",
        "illuminate/redis": "^9.17",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "^9.0",
        "laravelista/lumen-vendor-publish": "9.0",
        "league/flysystem": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mazfreelance/laravel-command-generator": "^2.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "predis/predis": "^2.0",
        "psr/simple-cache": "1.0",
        "pusher/pusher-php-server": "^7.0",
        "rogervila/lumen-rate-limiting": "^1.1",
        "sentry/sentry-laravel": "^2.12",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sluggable": "^3.4",
        "tucker-eric/eloquentfilter": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/mazfreelance/lumen-vendor-publish"
    }]
}
