{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "anik/form-request": "@dev",
        "bensampo/laravel-enum": "^6.4",
        "dusterio/lumen-passport": "@dev",
        "flipbox/lumen-generator": "^9.2",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/mail": "^10.15",
        "illuminate/notifications": "^10.15",
        "illuminate/redis": "^10.15",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "^10.0",
        "league/flysystem": "^3.15",
        "maatwebsite/excel": "^3.1",
        "mazfreelance/laravel-command-generator": "^2.2",
        "mazfreelance/lumen-vendor-publish": "^10.0",
        "psr/simple-cache": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "rogervila/lumen-rate-limiting": "^1.2",
        "sentry/sentry-laravel": "^3.6",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-sluggable": "^3.5",
        "tucker-eric/eloquentfilter": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mazfreelance/form-request.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/lumen-passport.git"
        }
    ]
}
